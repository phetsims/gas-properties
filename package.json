{
  "name": "gas-properties",
  "version": "1.2.0-dev.2",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/phetsims/gas-properties.git"
  },
  "devDependencies": {
    "grunt": "~1.5.3"
  },
  "phet": {
    "requirejsNamespace": "GAS_PROPERTIES",
    "simulation": true,
    "runnable": true,
    "phetLibs": [
      "bamboo",
      "twixt"
    ],
    "supportedBrands": [
      "phet",
      "phet-io",
      "adapted-from-phet"
    ],
    "simFeatures": {
      "supportsDynamicLocale": true,
      "supportsSound": true,
      "supportsInteractiveDescription": true,
      "colorProfiles": [
        "default",
        "projector"
      ]
    },
    "phet-io": {
      "compareDesignedAPIChanges": true,
      "wrappers": [
        "phet-io-sim-specific/repos/gas-properties/wrappers/codap"
      ]
    },
    "supportsOutputJS": true,
    "published": true,
    "screenNameKeys": [
      "GAS_PROPERTIES/screen.ideal",
      "GAS_PROPERTIES/screen.explore",
      "GAS_PROPERTIES/screen.energy",
      "GAS_PROPERTIES/screen.diffusion"
    ]
  }
}